# HomeCenter Environment Variables
# Copy this file to .env and fill in your values
# These values work for both local development and Docker
# Format: Use double underscores (__) which work for both scenarios

# Admin Credentials
Admin__Username=admin
Admin__Password=your-secure-password-here

# AI Configuration (OpenRouter)
AI__Provider=OpenRouter
AI__ApiKey=your-openrouter-api-key-here
AI__Enabled=true
AI__Model=openrouter/free
AI__BaseUrl=https://openrouter.ai/api/v1

# Qwen Configuration (Optional)
Qwen__ApiKey=your-qwen-api-key-here
Qwen__Enabled=false
Qwen__Model=qwen-turbo
Qwen__BaseUrl=https://dashscope-intl.aliyuncs.com/api/v1
